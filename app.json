{
  "expo": {
    "name": "LittleFlight",
    "slug": "little-flight",
    "owner": "kenmav",
    "version": "2.0.0",
    "scheme": "littleflight",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiryboy.littleflight"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE"
      ],
      "package": "com.tiryboy.littleflight"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
